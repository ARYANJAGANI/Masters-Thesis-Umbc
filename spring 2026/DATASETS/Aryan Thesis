     \"Page Contents\": page_text_clean\n","        })\n","\n","# Create final table (one row per page)\n","final_df = pd.DataFrame(rows).sort_values([\"Book name\", \"Page ID\"])\n","\n","# Save in the exact format you showed\n","output_xlsx = \"/content/Book_Content_Page_Level.xlsx\"\n","output_csv  = \"/content/Book_Content_Page_Level.csv\"\n","\n","final_df.to_excel(output_xlsx, index=False)\n","final_df.to_csv(output_csv, index=False)\n","\n","print(\"✅ Saved:\")\n","print(output_xlsx)\n","print(output_csv)\n","\n","final_df.head()\n"],"metadata":{"id":"IKaQoL7gk2cU","colab":{"base_uri":"https://localhost:8080/","height":258},"executionInfo":{"status":"ok","timestamp":1766775931879,"user_tz":300,"elapsed":133,"user":{"displayName":"Aryan Jagani","userId":"01939686628835301547"}},"outputId":"be02b500-10b5-4a11-be1d-12abd7388fdb"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["✅ Saved:\n","/content/Book_Content_Page_Level.xlsx\n","/content/Book_Content_Page_Level.csv\n"]},{"output_type":"execute_result","data":{"text/plain":["                                   Book name                        Author  \\\n","22  Brown Bear, Brown Bear, What Do You See?  Bill Martin Jr. & Eric Carle   \n","23  Brown Bear, Brown Bear, What Do You See?  Bill Martin Jr. & Eric Carle   \n","24  Brown Bear, Brown Bear, What Do You See?  Bill Martin Jr. & Eric Carle   \n","25  Brown Bear, Brown Bear, What Do You See?  Bill Martin Jr. & Eric Carle   \n","26  Brown Bear, Brown Bear, What Do You See?  Bill Martin Jr. & Eric Carle   \n","\n","    Page ID                                      Page Contents  \n","22        1  CELEBRATING Bill Martin Jr / Eric Carle Brown ...  \n","23        2  Brown Bear, Brown Bear, What do you see? I see...  \n","24        3  Red Bird, Red Bird, What do you see? I see a y...  \n","25        4  Yellow Duck, Yellow Duck, What do you see? I s...  \n","26        5  Blue Horse, Blue Horse, What do you see? I see...  "],"text/htm